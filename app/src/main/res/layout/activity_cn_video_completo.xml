<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_cn_video_completo"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="supercrack.sigmamoviles.com.ama.Activity.CN_VideoCompletoActivity">

    <VideoView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/video_cnactivityvideocompleto_videc"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:text="@string/salir"
        android:layout_height="wrap_content"
        android:id="@+id/btn_cnactivityvideocompleto_salir"
        app:layout_constraintRight_toRightOf="@+id/video_cnactivityvideocompleto_videc"
        android:layout_marginEnd="24dp"
        app:layout_constraintLeft_toLeftOf="@+id/video_cnactivityvideocompleto_videc"
        app:layout_constraintTop_toTopOf="@+id/video_cnactivityvideocompleto_videc"
        android:layout_marginTop="16dp"
        android:background="@drawable/boton_redondo"
        app:layout_constraintHorizontal_bias="1.0"
        android:layout_width="100dp"
        app:layout_constraintBottom_toBottomOf="@+id/video_cnactivityvideocompleto_videc"
        app:layout_constraintVertical_bias="0.0"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textSize="18sp"
        android:textColor="@color/blanco_ama" />
</android.support.constraint.ConstraintLayout>
