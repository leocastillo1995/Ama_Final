        <activity android:name=".Activity.SCN_PreguntasActivity" android:screenOrientation="portrait"/>
        <activity
            android:name=".Activity.CN_VideoCompletoActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".Activity.CN_FinalizarActivity" android:screenOrientation="portrait"/>
        <activity android:name=".Activity.SCN_EdadActivity" android:screenOrientation="portrait"/>
        <activity android:name=".Activity.SCN_HabilidadActivity" android:screenOrientation="portrait"/>
        <activity android:name=".Activity.SCN_ListaVideoActivity" android:screenOrientation="portrait"/>
        <activity
