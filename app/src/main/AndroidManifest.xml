        <activity android:name=".Activity.SCN_InicioActivity" android:screenOrientation="portrait"/>
        <activity
            android:name=".Activity.CN_RegistroUsuarioActivity"
            android:parentActivityName=".Activity.CN_InicioActivity" android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activity.CN_InicioActivity" />
        </activity>
        <activity
            android:name=".Activity.CN_RegistroSexoActivity"
