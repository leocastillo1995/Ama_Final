        </activity>
        <activity android:name=".Activity.CN_EditarPerfilActivity" android:screenOrientation="portrait"/>
        <activity android:name=".Activity.CN_TemaForoDetalleActivity" android:screenOrientation="portrait"/>
        <activity
            android:name=".Activity.CN_ComentarioForoActivity"
            android:parentActivityName=".Activity.CN_TemaForoDetalleActivity" android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activity.CN_TemaForoDetalleActivity" />
        </activity>
        <activity
            android:name=".Activity.CN_Pregunta1Activity"
            android:parentActivityName=".Activity.CN_MenuActivity" android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activity.CN_MenuActivity" />
        </activity>
        <activity android:name=".Activity.CN_PreguntasActivity" android:screenOrientation="portrait"/>
        <activity android:name=".Activity.CN_EdadActivity" android:screenOrientation="portrait"/>
        <activity android:name=".Activity.HabilidadesActivity" android:screenOrientation="portrait"/>
