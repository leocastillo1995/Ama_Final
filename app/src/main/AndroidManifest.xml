<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="supercrack.sigmamoviles.com.ama">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.CN_InicioActivity" />
        <activity
            android:name=".Activity.CN_MenuActivity"
            android:label="@string/title_activity_cn__menu"
            android:theme="@style/AppTheme" />
        <activity android:name=".Activity.SCN_InicioActivity" />
        <activity
            android:name=".Activity.CN_RegistroUsuarioActivity"
            android:parentActivityName=".Activity.CN_InicioActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activity.CN_InicioActivity" />
        </activity>
        <activity android:name=".Activity.CN_RegistroSexoActivity"
                  android:parentActivityName=".Activity.CN_RegistroUsuarioActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activity.CN_RegistroUsuarioActivity" />

        </activity>
    </application>

</manifest>