        </activity>
        <activity
            android:name=".Activity.CN_LogueoUsuarioActivity"
            android:parentActivityName=".Activity.CN_InicioActivity" android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
